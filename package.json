{
  "name": "tweetboard-api",
  "version": "0.0.1",
  "description": "RESTFUL API for TweetBoard",
  "main": "dist",
  "scripts": {
    "dev": "TYPE=development nodemon -w src --exec \"npm run lint && babel-node src --presets es2015,stage-0\"",
    "build": "TYPE=production babel src -D -d dist --ignore **/*.spec.js --presets es2015,stage-0",
    "start": "TYPE=production node dist",
    "prestart": "npm run -s build",
    "lint": "eslint src",
    "pretest": "rm -rf dist | TYPE=test babel src -s -D -d dist --presets es2015,stage-0",
    "test": "TYPE=test mocha $(find dist -name '*.spec.js')"
  },
  "repository": "ardaogulcan/TweetBoard-SWE573",
  "private": true,
  "author": "Arda Pekta≈ü <ardaogulcan@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "express": "^4.13.3",
    "express-boom": "^2.0.0",
    "helmet": "^3.5.0",
    "jsonwebtoken": "^7.4.0",
    "lodash": "^4.17.4",
    "mongoose": "^4.9.1",
    "mongoose-slug-hero": "^1.0.8",
    "oauth": "^0.9.15",
    "randomstring": "^1.1.5",
    "request": "^2.81.0",
    "request-promise-native": "^1.0.3",
    "slug": "^0.9.1",
    "validator": "^7.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "eslint": "^3.1.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "mocha": "^3.2.0",
    "nodemon": "^1.9.2"
  }
}
